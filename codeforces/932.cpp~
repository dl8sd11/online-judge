#include<bits/stdc++.h>
using namespace std;
int ans[1000003];
int main() {
  int n,a,b;
  cin>>n>>a>>b;
  int i,j;
  for (i=1;i*a<=n;i++) {
    if ((n-a*i)%b==0) {
      j = (n-a*i)/b;
    }
  }
  for (int k=1;k<=i*a;k++) {
    ans[k] = (k-1+i)%(a*i)+1;
  }
  for (int k=i*a+1;k<=n;k++) {
    ans[k] = (k-1+j)%(b*j)+1;
  }
  for (int i=1;i<=n;i++) cout<<ans[i]<<' ';
}
